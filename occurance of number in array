// number of occurunce of an array element
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
     
     int sum=0;
     int[] arr= { 12,2,83,36,9,45,36,28,83,63,36,12,63,83,2};
     System.out.println("Enter a number to check number of Occurence in array");
     int num=sc.nextInt();
     int count=0;
     for(int i=0; i<arr.length;i++) {
       if(arr[i]==num){
           count++;
       }
     }
     System.out.println("the number of times your number is Occured is : "+ count);
     
       
    
    }
}


