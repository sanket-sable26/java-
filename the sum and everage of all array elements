class Main {
    public static void main(String[] args) {
     
     int sum=0;
     int[] arr= { 12,2,83,36,9,45,36,28};
     for(int i=0; i<arr.length;i++) {
       sum=sum+arr[i];
     }
       System.out.println("the sum  of all arrry elements is : "+ sum );
       int avg ;
       avg= sum/arr.length ;
    System.out.println("the average of all array elements is :"+ avg );
    }
}


